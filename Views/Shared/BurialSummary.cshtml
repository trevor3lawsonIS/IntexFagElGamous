@{
}

@model Burialmain

<div class="card card-outline-primary m-1 p-1">
    <div class="bg-faded p-1">
        <h4>
            @Model.Id
            <span class="badge badge-pill badge-primary" style="float:right">
                <small>@Model.Northsouth</small>
            </span>
        </h4>
    </div>
    <div class="card-text p-1">Sex: @Model.Sex</div>
    <div class="card-text p-1">Head Direction: @Model.Headdirection</div>
    <div class="card-text p-1">Depth: @Model.Depth</div>
</div>
